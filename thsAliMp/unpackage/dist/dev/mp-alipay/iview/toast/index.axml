<view><block a:if="{{visible&&mask}}"><view data-event-opts="{{[['tap',[['handleHide',['$event']]]]]}}" class="i-toast-mask" onTap="__e"></view></block><block a:if="{{visible}}"><view class="i-class i-toast"><block a:if="{{type!=='default'}}"><block><view class="i-toast-type"><block a:if="{{type==='success'}}"><i-icon vue-id="4763f971-1" i-class="i-toast-icon" type="success" onVueInit="__l"></i-icon></block><block a:else><block a:if="{{type==='warning'}}"><i-icon vue-id="4763f971-2" i-class="i-toast-icon" type="prompt" onVueInit="__l"></i-icon></block><block a:else><block a:if="{{type==='error'}}"><i-icon vue-id="4763f971-3" i-class="i-toast-icon" type="delete" onVueInit="__l"></i-icon></block><block a:else><block a:if="{{type==='loading'}}"><view class="i-toast-icon i-toast-loading"></view></block></block></block></block></view></block></block><block a:else><block><block a:if="{{icon}}"><i-icon vue-id="4763f971-4" i-class="i-toast-icon" type="{{icon}}" onVueInit="__l"></i-icon></block><block a:if="{{image}}"><image class="i-toast-image" src="{{image}}" mode="aspectFit"></image></block></block></block><block a:if="{{content}}"><view class="i-toast-content">{{content}}</view></block></view></block></view>