.popUpWrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 2000;
  background-color: rgba(0, 0, 0, 0.4);
}
.popUpWrap .userWrap {
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 2000;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  width: 560rpx;
  height: 426rpx;
  border-radius: 0 0 16rpx 16rpx;
  background-color: transparent;
}
.popUpWrap .userWrap .headWrap {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 128rpx;
  z-index: 2010;
  border-radius: 16rpx 16rpx 0 0;
  margin-bottom: -16rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.popUpWrap .userWrap .headWrap .headIcon {
  width: 112rpx;
  height: 112rpx;
  border-radius: 50%;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  z-index: 2013;
}
.popUpWrap .userWrap .headWrap .headBg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  z-index: 2011;
}
.popUpWrap .userWrap .content {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 314rpx;
  padding-top: 16rpx;
  background-color: #fff;
  border-radius: 16rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.popUpWrap .userWrap .content .item {
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
}
.popUpWrap .userWrap .content .userName {
  padding: 0 20rpx;
  font-size: 40rpx;
  border: 1px solid transparent;
}
.popUpWrap .userWrap .content .btn_one_wrap {
  position: relative;
  width: 464rpx;
  height: 88rpx;
  border-radius: 44rpx;
  font-size: 30rpx;
  color: #f00;
  text-align: center;
  line-height: 88rpx;
}
.popUpWrap .userWrap .content .btn_one_wrap .text {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  font-size: 30rpx;
  color: #fff;
  background-color: transparent;
}

