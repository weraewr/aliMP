<view data-event-opts="{{[['tap',[['changePopUpState',[1]]]]]}}" onTap="__e"><block a:if="{{showPop}}"><cover-view class="popUpWrap"><block a:if="{{popUpType=='userInfo'}}"><cover-view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="userWrap" catchTap="__e"><view class="content"><cover-view class="userName item">{{loginInfo.account}}</cover-view><cover-view class="btn_one_wrap"><image class="btn_one item" src="../../static/popUpBtn.png" data-event-opts="{{[['tap',[['exitLogin',['$event']]]]]}}" onTap="__e"></image><view data-event-opts="{{[['tap',[['exitLogin',['$event']]]]]}}" class="text" onTap="__e">退出登录</view></cover-view></view><view class="headWrap"><image class="headBg" src="../../static/headBg.png"></image><block a:if="{{headUrl}}"><image class="headIcon" src="{{headUrl}}"></image></block></view></cover-view></block></cover-view></block></view>