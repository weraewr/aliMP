<view class="searchstock"><view class="searchBox"><view class="searchWrap btn"><block a:if="{{showState}}"><view class="iconWrap"><image class="searchIcon" src="../../static/search.png"></image><text>搜股票名称/股票代码</text></view></block><view class="inputSearch"><input type="text" focus="{{true}}" placeholder="{{searchText}}" data-event-opts="{{[['focus',[['hidIcon',['$event']]]],['blur',[['showIcon',['$event']]]],['input',[['__set_model',['','searchContent','$event',[]]],['inputHandle',['$event']]]]]}}" value="{{searchContent}}" onFocus="__e" onBlur="__e" onInput="__e"/></view></view></view><view class="paddingBox"></view><block a:if="{{!searchContent}}"><view class="hotSrarch"><block a:if="{{hotData.length>0}}"><view class="hotSearchWrap"><view class="tit">大家都在搜</view><view class="hotItem" a:for="{{$root.l0}}" a:for-item="item" a:for-index="index" a:key="name"><view data-event-opts="{{[['tap',[['jump',['$0'],[[['hotData','name',item.$orig.name]]]]]]]}}" class="info" onTap="__e"><view class="rank">{{index+1}}</view><view class="stockName">{{item.$orig.name}}</view><view class="{{(('precent')+' '+(item.$orig&&item.$orig.gain<0?'down':'up'))}}">{{item.f0}}</view></view><block a:if="{{loginInfo.account}}"><view data-event-opts="{{[['tap',[['optionChange',['$0'],[[['hotData','name',item.$orig.name]]]]]]]}}" class="btn" onTap="__e">{{item.$orig.optional?'删自选':'加自选'}}</view></block><block a:else><view class="btn"><button style="{{'background-color:'+('transparent')+';'}}" id="btnLogin" data-event-opts="{{[['tap',[['getPhoneNumber',['$event']]]]]}}" onTap="__e">加自选</button></view></block></view></view></block></view></block><block a:else><block a:if="{{searchData.length>0}}"><view class="searchContent"><view class="searchItem" a:for="{{$root.l1}}" a:for-item="item" a:for-index="__i0__" a:key="name"><view data-event-opts="{{[['tap',[['jump',['$0'],[[['searchData','name',item.$orig.name]]]]]]]}}" class="leftWrap" onTap="__e"><view class="stockName"><view class="name"><rich-text nodes="{{item.m0}}"></rich-text></view><view class="code"><rich-text nodes="{{item.m1}}"></rich-text></view></view><view class="{{(('price')+' '+(item.$orig.newPrice<0?'down':'up'))}}">{{item.f1}}</view><view class="{{((('precent')+' '+(item.$orig.gain<0?'down':''))+' '+(item.$orig.gain>0?'up':''))}}">{{item.f2}}</view></view><block a:if="{{loginInfo.account}}"><view data-event-opts="{{[['tap',[['optionChange',['$0'],[[['searchData','name',item.$orig.name]]]]]]]}}" class="{{(('option')+' '+(item.$orig.optional?'del':''))}}" onTap="__e">{{item.$orig.optional?'删自选':'加自选'}}</view></block><block a:else><view class="option"><button style="{{'background-color:'+('transparent')+';'}}" id="btnLogin" data-event-opts="{{[['tap',[['getPhoneNumber',['$event']]]]]}}" onTap="__e">加自选</button></view></block></view></view></block><block a:else><block a:if="{{searchData.length==0&&!searching&&searchContent}}"><view class="searchContent"><view class="noContent">找不到任何匹配内容</view></view></block></block></block></view>